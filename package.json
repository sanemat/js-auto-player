{
  "devDependencies": {
    "babelify": "^5.0.4",
    "browser-sync": "^2.4.0",
    "browserify": "^9.0.3",
    "gulp": "^3.8.11",
    "gulp-load-plugins": "^0.9.0",
    "uniq": "^1.0.1",
    "watchify": "^2.6.2"
  },
  "scripts": {
    "server": "browser-sync start --files 'app/lib/*, app/index.html' --server 'app'",
    "watch": "./node_modules/watchify/bin/cmd.js ./app/src/app.js -o app/lib/bundle.js -v -d",
    "start": "npm run server & npm run watch",
    "build": "NODE_ENV=production browserify -d -e ./app/src/app.js -t [ babelify --experimental ] > ./app/lib/bundle.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "experimental": true
        }
      ]
    ]
  },
  "dependencies": {
    "flummox": "^3.2.1",
    "react": "^0.13.1",
    "react-youtube": "git://github.com/sanemat/react-youtube#chore/react-v0-13"
  }
}
